{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "rules": {
        "react/prop-types": 0,
        "no-underscore-dangle": ["error",
            {
                "allow": ["__REDUX_DEVTOOLS_EXTENSION__"]
            }],
        "arrow-parens": [2, "always"],
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "space-infix-ops": 0,
        "no-unused-vars": [1, {"vars": "local", "args": "none"}],
        "default-case": 0,
        "no-else-return": 0,
        "no-param-reassign": 0,
        "quotes": 0
    }
}
