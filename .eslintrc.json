{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "rules": {
        "no-underscore-dangle": ["error",
            {
                "allow": ["__REDUX_DEVTOOLS_EXTENSION__"]
            }],
        "arrow-parens": [2, "always"],
        "no-param-reassign": 0
    }
}
